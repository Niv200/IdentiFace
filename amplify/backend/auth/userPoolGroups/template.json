{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "authdevUserPoolId": {
            "Type": "String",
            "Default": "authdevUserPoolId"
        },
        
        "authdevAppClientID": {
            "Type": "String",
            "Default": "authdevAppClientID"
        },
        "authdevAppClientIDWeb": {
            "Type": "String",
            "Default": "authdevAppClientIDWeb"
        },
        "AuthRoleArn": {
            "Type": "String"
        },
        "UnauthRoleArn": {
            "Type": "String"
        },
        "env": {
            "Type": "String"
        }
    },
    "Conditions": {
        "ShouldNotCreateEnvResources": {
            "Fn::Equals": [
                {
                    "Ref": "env"
                },
                "NONE"
            ]
        }
    },
    "Resources": {
    
        "adminGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "admin",
              "Precedence" : 1,
              
              "UserPoolId" : {
                "Ref": "authdevUserPoolId"
              }
            }
        },
        
    
    
    },
    
}
